<p id="notice"><%= notice %></p>

<h1>Campaigns</h1>

<table class="table table-striped">
  <thead>
  <tr>
    <th>Name</th>
    <th>Candidate</th>
    <th>Score</th>
    <th>Invalid score</th>
  </tr>
  </thead>

  <tbody>
  <% @campaigns.each do |campaign| %>
      <tr>
        <td><%= campaign.name %></td>
        <td><%= campaign.candidate %></td>
        <td><%= campaign.score %></td>
        <td><%= campaign.invalid_score %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<%= link_to 'Back', summary_path %>
